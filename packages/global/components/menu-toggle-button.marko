<marko-web-browser-component
  name="GlobalMenuToggleButton"
  props={
    className: input.class,
    targets: input.targets,
    toggleClass: input.toggleClass,
    iconModifiers: input.iconModifiers,
    iconName: input.iconName,
    expandedIconName: input.expandedIconName,
  }
  tag="button"
  class=input.class
  attrs={ type: "button", "aria-label": "Toggle Menu" }
  hydrate=true
>
  <marko-web-icon name=input.iconName modifiers=input.iconModifiers />
</marko-web-browser-component>
